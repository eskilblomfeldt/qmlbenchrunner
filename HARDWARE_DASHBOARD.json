          "span": 6,
          "stack": false,
          "steppedLine": false,
          "targets": [
            {
              "alias": "$tag_branch $tag_hardwareId",
              "dsType": "influxdb",
              "groupBy": [
                {
                  "params": [
                    "$interval"
                  ],
                  "type": "time"
                },
                {
                  "params": [
                    "branch"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "hardwareId"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "none"
                  ],
                  "type": "fill"
                }
              ],
              "hide": false,
              "measurement": "benchmarks",
              "policy": "default",
              "query": "SELECT mean(\"mean\") FROM \"benchmarks\" WHERE $timeFilter GROUP BY time($interval) fill(null)",
              "rawQuery": false,
              "refId": "A",
              "resultFormat": "time_series",
              "select": [
                [
                  {
                    "params": [
                      "mean"
                    ],
                    "type": "field"
                  },
                  {
                    "params": [],
                    "type": "mean"
                  }
                ]
              ],
              "tags": [
                {
                  "key": "benchmark",
                  "operator": "=~",
                  "value": "/^$benchmark$/"
                },
                {
                  "condition": "AND",
                  "key": "branch",
                  "operator": "=~",
                  "value": "/^$branch$/"
                },
                {
                  "condition": "AND",
                  "key": "hardwareId",
                  "operator": "=~",
                  "value": "/^$hardwareId$/"
                }
              ]
            }
          ],
          "thresholds": [],
