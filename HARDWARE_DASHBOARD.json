            {
              "alias": "/CoV.+/",
              "stack": false,
              "yaxis": 2,
              "zindex": 3
            },
            {
              "alias": "/Result.+/",
              "fill": 0
            }
          ],
          "span": 6,
          "stack": false,
          "steppedLine": false,
          "targets": [
            {
              "alias": "Result ($tag_branch $tag_hardwareId)",
              "dsType": "influxdb",
              "groupBy": [
                {
                  "params": [
                    "$interval"
                  ],
                  "type": "time"
                },
                {
                  "params": [
                    "branch"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "hardwareId"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "none"
                  ],
                  "type": "fill"
                }
              ],
              "hide": false,
              "measurement": "benchmarks",
              "policy": "default",
              "query": "SELECT mean(\"mean\") FROM \"benchmarks\" WHERE \"benchmark\" =~ /^$benchmark$/ AND \"branch\" =~ /^$branch$/ AND \"hardwareId\" =~ /^$hardwareId$/ AND $timeFilter GROUP BY time($interval), \"branch\", \"hardwareId\" fill(none)",
              "rawQuery": false,
              "refId": "A",
              "resultFormat": "time_series",
              "select": [
                [
                  {
                    "params": [
                      "mean"
                    ],
                    "type": "field"
                  },
                  {
                    "params": [],
                    "type": "mean"
                  }
                ]
              ],
              "tags": [
                {
                  "key": "benchmark",
                  "operator": "=~",
                  "value": "/^$benchmark$/"
                },
                {
                  "condition": "AND",
                  "key": "branch",
                  "operator": "=~",
                  "value": "/^$branch$/"
                },
                {
                  "condition": "AND",
                  "key": "hardwareId",
                  "operator": "=~",
                  "value": "/^$hardwareId$/"
                }
              ]
            },
            {
              "alias": "CoV ($tag_branch $tag_hardwareId)",
              "dsType": "influxdb",
              "groupBy": [
                {
                  "params": [
                    "$interval"
                  ],
                  "type": "time"
                },
                {
                  "params": [
                    "branch"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "hardwareId"
                  ],
                  "type": "tag"
                },
                {
                  "params": [
                    "none"
                  ],
                  "type": "fill"
                }
              ],
              "measurement": "benchmarks",
              "policy": "default",
              "refId": "B",
              "resultFormat": "time_series",
              "select": [
                [
                  {
                    "params": [
                      "coefficientOfVariation"
                    ],
                    "type": "field"
                  },
                  {
                    "params": [],
                    "type": "mean"
                  }
                ]
              ],
              "tags": [
                {
                  "key": "benchmark",
                  "operator": "=~",
                  "value": "/^$benchmark$/"
                },
                {
                  "condition": "AND",
                  "key": "branch",
                  "operator": "=~",
                  "value": "/^$branch$/"
                },
                {
                  "condition": "AND",
                  "key": "hardwareId",
                  "operator": "=~",
                  "value": "/^$hardwareId$/"
